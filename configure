    { echo "$as_me:3309: loading cache $cache_file" >&5
echo "loading cache $cache_file" >&6;}
    case $cache_file in
      [\\/]* | ?:[\\/]* ) . $cache_file;;
      *)                      . ./$cache_file;;
    esac
  fi
else
  { echo "$as_me:3317: creating cache $cache_file" >&5
echo "creating cache $cache_file" >&6;}
  >$cache_file
fi

# This can be used to rebuild libtool when needed
