           # Source tree
           echo $ac_given_srcdir/$f
         else
           # /dev/null tree
           {  echo "$as_me: error: cannot find input file \\\`$f'" >&2
  { false; exit; }; }
         fi;;
      esac
    done` || { false; exit; }
