# **************************************************************************
# Remove redundant options from certain option lists.

sim_ac_uniqued_list=
for sim_ac_item in -I$includedir $SOQT_EXTRA_CPPFLAGS; do
  if test x"$sim_ac_uniqued_list" = x; then
    sim_ac_uniqued_list="$sim_ac_item"
  else
    sim_ac_unique=true
    for sim_ac_uniq in $sim_ac_uniqued_list; do
      test x"$sim_ac_item" = x"$sim_ac_uniq" && sim_ac_unique=false
    done
    $sim_ac_unique && sim_ac_uniqued_list="$sim_ac_uniqued_list $sim_ac_item"
  fi
done
SOQT_EXTRA_CPPFLAGS=$sim_ac_uniqued_list

sim_ac_uniqued_list=
for sim_ac_item in -L$libdir $SOQT_EXTRA_LDFLAGS; do
  if test x"$sim_ac_uniqued_list" = x; then
    sim_ac_uniqued_list="$sim_ac_item"
  else
    sim_ac_unique=true
    for sim_ac_uniq in $sim_ac_uniqued_list; do
      test x"$sim_ac_item" = x"$sim_ac_uniq" && sim_ac_unique=false
    done
    $sim_ac_unique && sim_ac_uniqued_list="$sim_ac_uniqued_list $sim_ac_item"
  fi
done
SOQT_EXTRA_LDFLAGS=$sim_ac_uniqued_list

sim_ac_uniqued_list=
for sim_ac_item in $SOQT_LIBNAME $SOQT_EXTRA_LIBS; do
  if test x"$sim_ac_uniqued_list" = x; then
    sim_ac_uniqued_list="$sim_ac_item"
  else
    sim_ac_unique=true
    for sim_ac_uniq in $sim_ac_uniqued_list; do
      test x"$sim_ac_item" = x"$sim_ac_uniq" && sim_ac_unique=false
    done
    $sim_ac_unique && sim_ac_uniqued_list="$sim_ac_uniqued_list $sim_ac_item"
  fi
done
SOQT_EXTRA_LIBS=$sim_ac_uniqued_list

